<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/questionnaire_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.viizfo.test1ev.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_questionnaireFragment"
            app:destination="@id/questionnaireFragment" />
    </fragment>
    <fragment
        android:id="@+id/questionnaireFragment"
        android:name="com.viizfo.test1ev.QuestionnaireFragment"
        android:label="@string/questionnaire"
        tools:layout="@layout/fragment_questionnaire" >
        <action
            android:id="@+id/action_questionnaireFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_questionnaireFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_questionnaireFragment_self"
            app:destination="@id/questionnaireFragment" />
        <argument
            android:name="QuestionInfo"
            app:argType="com.viizfo.test1ev.controller.QuestionnaireFragmentData" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.viizfo.test1ev.ResultFragment"
        android:label="@string/result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_resultFragment_to_questionnaireFragment"
            app:destination="@id/questionnaireFragment" />
        <argument
            android:name="ResultInfo"
            app:argType="com.viizfo.test1ev.controller.ResultFragmentData" />
    </fragment>
</navigation>